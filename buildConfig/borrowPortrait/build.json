{
  "productName": "蓝鲸图书馆借还机-竖屏版",
  "appId": "com.bluewhale.borrow_portrait",
  "buildVersion": "1.0.0-borrow_portrait",
  "directories": {
    "output": "build/borrow_portrait"
  },
  "files": [
    "dist/electron/**/*"
  ],
  "asar": false,
  "nsis": {
    "oneClick": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "./static/ico.ico",
    "uninstallerIcon": "./static/ico.ico",
    "installerHeaderIcon": "./static/ico.ico",
    "include": "buildConfig/borrowPortrait/installer.nsh"
  },
  "electronDownload": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  },
  "extraResources": [
    {
      "from": "./extraResources/devices/common/",
      "to": "extraResources/modules/"
    },
    {
      "from": "./extraResources/devices/baidu/",
      "to": "extraResources/"
    },
    {
      "from": "./buildConfig/borrowPortrait/config/",
      "to": "config/"
    },
    {
      "from": "./extraResources/icon/",
      "to": "extraResources/icon/"
    },
    {
      "from": "./extraResources/modules/",
      "to": "extraResources/modules/"
    }
  ],
"win": {
"icon": "build/icons/icon.ico",
"target": [
  "nsis"
]
}
}