{
  "productName": "蓝鲸图书馆主软件",
  "appId": "com.bluewhale.work_deck_main",
  "buildVersion": "1.0.0-workdeckmain",
  "directories": {
    "output": "build/work_deck_main"
  },
  "files": [
    "dist/electron/**/*"
  ],
  "asar": false,
  "nsis": {
    "oneClick": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "./static/ico.ico",
    "uninstallerIcon": "./static/ico.ico",
    "installerHeaderIcon": "./static/ico.ico",
    "include": "buildConfig/workdeckMain/installer.nsh"
  },
  "electronDownload": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  },
  "extraResources": [
    {
      "from": "./buildConfig/workdeckMain/config/",
      "to": "config"
    },
    {
      "from": "./extraResources/icon/",
      "to": "extraResources/icon/"
    }
  ],
"win": {
"icon": "build/icons/icon.ico",
"target": [
"nsis"
]
}
}